{
  "scripts": {
    "tsc": "npx tsc",
    "test": "jest",
    "lint:staged": "eslint --fix --max-warnings 0",
    "spell-check": "cspell lint -c ./.cspell/cspell.cjs --gitignore --show-suggestions",
    "prepare": "husky",
    "checked": "npx npm-run-all --parallel --continue-on-error lint:staged spell-check",
    "node:run": "ts-node src/logic/$(git branch --show-current | cut -d'/' -f1)/execute.ts"
  },
  "lint-staged": {
    "src/**/*.ts": [
      "eslint --fix --max-warnings 0",
      "npm run spell-check"
    ]
  },
  "devDependencies": {
    "@eslint/js": "^9.31.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.0.15",
    "@typescript-eslint/eslint-plugin": "^8.37.0",
    "@typescript-eslint/parser": "^8.37.0",
    "cspell": "^8.17.3",
    "eslint": "^9.31.0",
    "globals": "^16.3.0",
    "jest": "^30.0.4",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.37.0"
  },
  "dependencies": {
    "lint-staged": "^16.1.2"
  }
}
